@model IEnumerable<AddressBook.DAL.ContactType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Add")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContactTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ContactTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ContactTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ContactTypeId })
        </td>
    </tr>
}

</table>
